<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd" >
<properties>
    <comment>개발(shin)</comment>

    <!--DB-->
    <entry key="JDBC.POSTGRESQL.DRIVER">org.postgresql.Driver</entry>
    <entry key="JDBC.POSTGRESQL.URL">jdbc:postgresql://localhost:5432/vdrs</entry>
    <entry key="JDBC.POSTGRESQL.USERNAME">vdrs</entry>
    <entry key="JDBC.POSTGRESQL.PASSWORD">vdrs00</entry>

    <entry key="SFTP_IP">192.168.0.27</entry>
    <entry key="SFTP_PORT">22</entry>
    <entry key="SFTP_ID">vdrs</entry>
    <entry key="SFTP_PW">vdrs00</entry>

    <!--저장될 로컬폴더 ex) C:/um/ftpTest-->
    <entry key="SFTP_TARGET">F:/test/sftpTest/um</entry>

    <entry key="SFTP_TEMPFOLDER">F:/test/sftpTest/umTemp</entry>

    <!--SFTP 로컬 폴더와 동일하게 설정-->
    <entry key="API_ROOT">F:/test/sftpTest/um</entry>

    <!--원격 폴더구조(/에서 기상장파일까지경로)-->
    <entry key="REMOTE_ROOT">/remote/</entry>
    <entry key="MODE">default</entry>

    <entry key="CMD.MOVE">move</entry>

</properties>